<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane minHeight="700.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.createUserController">
    <children>
        <GridPane prefHeight="700.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="589.0" minHeight="10.0" prefHeight="235.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="589.0" minHeight="10.0" prefHeight="173.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="276.0" minHeight="10.0" prefHeight="265.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Create new user">
                    <font>
                        <Font size="36.0" />
                    </font>
                    <GridPane.margin>
                        <Insets left="171.0" />
                    </GridPane.margin>
                </Label>
                <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#createUser" prefHeight="55.0" prefWidth="98.0" text="Confirm" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="20.0" left="200.0" />
                    </GridPane.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#back" prefHeight="56.0" prefWidth="95.0" text="  Back  " GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="20.0" left="320.0" />
                    </GridPane.margin>
                </Button>
                <Label fx:id="userIDLabel" prefHeight="50.0" prefWidth="331.0" text="Your userID is" visible="false" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets left="145.0" top="100.0" />
                    </GridPane.margin>
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
                <Label text="Username:" GridPane.rowIndex="1">
                    <font>
                        <Font size="20.0" />
                    </font>
                    <GridPane.margin>
                        <Insets left="120.0" />
                    </GridPane.margin>
                </Label>
                <TextField fx:id="userName" maxWidth="200.0" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets left="230.0" />
                    </GridPane.margin>
                </TextField>
            </children>
        </GridPane>
    </children>
</AnchorPane>
